PRECOMPILER_ARGS=--api=4

@dynamic_module_makefile@

stringtree.o: $(SRCDIR)/stringtree.c

inttree.o: $(SRCDIR)/inttree.c

floattree.o: $(SRCDIR)/floattree.c

glue.o: $(SRCDIR)/glue.c

$(SRCDIR)/stringtree.c: $(CMOD_HEADERS)
$(SRCDIR)/inttree.c: $(CMOD_HEADERS)
$(SRCDIR)/floattree.c: $(CMOD_HEADERS)

@dependencies@
