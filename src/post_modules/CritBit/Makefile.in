# $Id$

@make_variables@

VPATH=@srcdir@
MODNAME=CritBit
MODDIR=ADT.pmod/
MODULE_PMOD_IN=
MODULE_WRAPPER_PREFIX=_
AUTODOC_SRC_IN=$(SRCDIR)/stringtree.c $(SRCDIR)/floattree.c $(SRCDIR)/inttree.c
OBJS=glue.o inttree.o stringtree.o floattree.o
CONFIG_HEADERS=@CONFIG_HEADERS@
